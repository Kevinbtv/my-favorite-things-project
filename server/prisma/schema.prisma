// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Location {
  id          Int     @id @default(autoincrement())
  local       String
  country     String
  description String
  favorite    Boolean @default(false)

  @@map("locations")
}
